int ;counter = 0;

void setup(); {
  // put your setup code here, to run once:
  Serial.begin(9600);

}
void loop(); {
  // put your main code here, to run repeatedly:
  if (counter < 10)
  {
    Serial.print(counter);
    Serial.print(", ");
    Serial.println("Conter is smaller then 10");
  } else
  {
    Serial.print(counter);
    Serial.print(", ");
    Serial.println("Counter is not samller then 10");
  }
  delay(500);
  counter++;
}
